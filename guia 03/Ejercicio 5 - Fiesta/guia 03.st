Object subclass: #AdministradorFiesta	instanceVariableNames: 'nombre colBar colBebedor'	classVariableNames: ''	package: 'guia 03'!!AdministradorFiesta methodsFor: 'as yet unclassified' stamp: 'GabrielF 5/3/2021 19:27'!verificarCervezaPreferida: unBebedor conNombreCerveza: nombreCerveza^unBebedor esPreferida: nombreCerveza.! !!AdministradorFiesta methodsFor: 'as yet unclassified' stamp: 'GabrielF 5/3/2021 20:44'!calcularPorcentajeBares: unaCerveza	| cont resultado |	cont := 0.	colBar		do: [ :each | 			(each verificarCerveza: unaCerveza)				ifTrue: [ cont := cont + 1 ] ].	resultado := cont // colBar size * 100.	^ resultado! !!AdministradorFiesta methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:25'!agregarBebedor: unBebedorcolBebedor add: unBebedor.! !!AdministradorFiesta methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:27'!agregarBar: unBarcolBar add: unBar.! !!AdministradorFiesta methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:07'!setNombre: unNombre^unNombre.! !!AdministradorFiesta methodsFor: 'initialization' stamp: 'GabrielF 4/30/2021 19:26'!inicializarAdminConNombre: unNombre	nombre := unNombre.	colBebedor := OrderedCollection new.	colBar := OrderedCollection new! !!AdministradorFiesta methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:07'!getNombre^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AdministradorFiesta class	instanceVariableNames: ''!!AdministradorFiesta class methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:07'!crearAdminConNombre: unNombre	|admin|		admin := self new.		admin inicializarAdminConNombre: unNombre.		^admin.! !Object subclass: #Bar	instanceVariableNames: 'nombre ubicacion colCerveza'	classVariableNames: ''	package: 'guia 03'!!Bar methodsFor: 'as yet unclassified' stamp: 'GabrielF 5/3/2021 20:17'!verificarCerveza: unaCerveza	| bar resultado |	bar := colCerveza detect: [ :each | each tieneCerveza: unaCerveza ] ifNone:[^false].	bar ~= nil		ifTrue: [ resultado := true ].	^ resultado! !!Bar methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:26'!agregarCerveza: unaCerveza colCerveza add: unaCerveza.! !!Bar methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:20'!setUbicacion: unaUbicacionubicacion := unaUbicacion.! !!Bar methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:20'!setNombre: unNombrenombre := unNombre.! !!Bar methodsFor: 'initialization' stamp: 'GabrielF 4/30/2021 19:18'!inicializarBarConNombre: unNombre conUbicacion: unaUbicacionnombre := unNombre.ubicacion := unaUbicacion.colCerveza := OrderedCollection new.! !!Bar methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:20'!getNombre^nombre.! !!Bar methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:20'!getUbicacion^ubicacion.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Bar class	instanceVariableNames: ''!!Bar class methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:19'!crearBarConNombre: unNombre conUbicacion: unaUbicacion	|bar|		bar := self new.		bar inicializarBarConNombre: unNombre conUbicacion: unaUbicacion.		^bar.! !Object subclass: #Bebedor	instanceVariableNames: 'nombre apellido lugarDeProcedencia datosPersonales cervezaPreferida colBares'	classVariableNames: ''	package: 'guia 03'!!Bebedor methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:11'!getCervezaPreferida^cervezaPreferida! !!Bebedor methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:16'!getDatosPersonales^datosPersonales! !!Bebedor methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:11'!getLugarDeProcedencia^lugarDeProcedencia ! !!Bebedor methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:10'!getApellido^apellido.! !!Bebedor methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:10'!getNombre^nombre.! !!Bebedor methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:15'!setCervezaPreferida: unaCervezaPreferidacervezaPreferida := unaCervezaPreferida! !!Bebedor methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:15'!setDatosPersonales: unDatosPerdatosPersonales := unDatosPer! !!Bebedor methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:15'!setLugarDeProcedencia: unLugarDeProcedencialugarDeProcedencia := unLugarDeProcedencia.! !!Bebedor methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:14'!setApellido: unApellidoapellido := unApellido.! !!Bebedor methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:12'!setNombre: unNombrenombre := unNombre! !!Bebedor methodsFor: 'initialization' stamp: 'GabrielF 4/30/2021 19:30'!inicializarBebConNombre: unNombre conApe: unApellido conLugarProc: unLugar conDatosPer: unDatosPer conCervPref: unaCerveza	nombre := unNombre.	apellido := unApellido.	lugarDeProcedencia := unLugar.	datosPersonales := unDatosPer.	cervezaPreferida := unaCerveza.	colBares := OrderedCollection new! !!Bebedor methodsFor: 'as yet unclassified' stamp: 'GabrielF 5/3/2021 19:27'!esPreferida: unNombreCerveza^unNombreCerveza == cervezaPreferida.! !!Bebedor methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:26'!agregarBar: unBarcolBares := unBar.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Bebedor class	instanceVariableNames: ''!!Bebedor class methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:31'!crearBebConNombre: unNombre conApe: unApellido conLugarProc: unLugar conDatosPer: unDatosPer conCervPref: unaCerveza	| bebedor |	bebedor := self new.	bebedor		inicializarBebConNombre: unNombre		conApe: unApellido		conLugarProc: unLugar		conDatosPer: unDatosPer		conCervPref: unaCerveza.	^ bebedor! !Object subclass: #Cerveza	instanceVariableNames: 'nombre'	classVariableNames: ''	package: 'guia 03'!!Cerveza methodsFor: 'getters' stamp: 'GabrielF 4/30/2021 19:04'!getNombre^nombre.! !!Cerveza methodsFor: 'initialization' stamp: 'GabrielF 4/30/2021 19:04'!inicializarCervConNombre: unNombrenombre := unNombre.! !!Cerveza methodsFor: 'setters' stamp: 'GabrielF 4/30/2021 19:04'!setNombre: unNombrenombre := unNombre.! !!Cerveza methodsFor: 'as yet unclassified' stamp: 'GabrielF 5/3/2021 19:57'!tieneCerveza: unaCerveza	^ unaCerveza getNombre == nombre! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Cerveza class	instanceVariableNames: ''!!Cerveza class methodsFor: 'as yet unclassified' stamp: 'GabrielF 4/30/2021 19:05'!crearCervConNombre: unNombre	|cerveza|		cerveza := self new.		cerveza inicializarCervConNombre: unNombre.		^cerveza.! !